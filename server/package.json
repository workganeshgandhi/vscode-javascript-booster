{
    "name": "vscode-javascript-booster-lang-server",
    "description": "JavaScript Booster Language Server",
    "version": "0.0.1",
    "author": "Stepan Burguchev <me@sburg.net>",
    "license": "MIT",
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.6.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/xsburg/vscode-javascript-booster.git"
    },
    "bugs": {
        "url": "https://github.com/xsburg/vscode-javascript-booster/issues"
    },
    "scripts": {
        "compile:dev": "webpack --mode none",
        "compile:test": "webpack --mode none",
        "compile:prod": "webpack --mode production",
        "test": "jest",
        "generate-ast-typings": "node ./typings/generate-ast-typings"
    },
    "dependencies": {
        "@babel/parser": "7.9.6",
        "fs-extra": "9.0.0",
        "ast-types": "0.12.3",
        "recast": "0.17.5",
        "jscodeshift": "xsburg/jscodeshift",
        "lodash": "4.17.15",
        "vscode-languageserver": "6.1.1",
        "vscode-languageserver-textdocument": "^1.0.1",
        "vscode-uri": "2.1.1"
    },
    "devDependencies": {
        "@types/fs-extra": "9.0.0",
        "@types/jest": "25.2.2",
        "@types/lodash": "4.14.151",
        "@types/node": "14.0.1",
        "coveralls": "3.1.0",
        "cpx": "1.5.0",
        "jest": "26.0.1",
        "prettier": "2.0.5",
        "react": "16.13.1",
        "rimraf": "3.0.2",
        "ts-jest": "26.0.0",
        "ts-loader": "^7.0.4",
        "typescript": "3.9.2",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "jest": {
        "testEnvironment": "node",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "/tests/.*\\.spec\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
