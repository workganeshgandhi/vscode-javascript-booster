{
    "name": "vscode-javascript-booster-lang-server",
    "description": "JavaScript Booster Language Server",
    "version": "0.0.1",
    "author": "Stepan Burguchev <me@sburg.net>",
    "license": "MIT",
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.6.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/xsburg/vscode-javascript-booster.git"
    },
    "bugs": {
        "url": "https://github.com/xsburg/vscode-javascript-booster/issues"
    },
    "scripts": {
        "compile:dev": "webpack --mode none",
        "compile:prod": "webpack --mode production",
        "test": "jest",
        "generate-ast-typings": "node ./typings/generate-ast-typings"
    },
    "dependencies": {
        "@babel/parser": "7.6.0",
        "fs-extra": "8.1.0",
        "ast-types": "0.12.3",
        "recast": "0.17.5",
        "jscodeshift": "xsburg/jscodeshift",
        "lodash": "4.17.15",
        "vscode-languageserver": "5.3.0-next.10",
        "vscode-uri": "2.0.3"
    },
    "devDependencies": {
        "@types/fs-extra": "8.0.0",
        "@types/jest": "24.0.18",
        "@types/lodash": "4.14.138",
        "@types/node": "12.7.5",
        "coveralls": "3.0.6",
        "cpx": "1.5.0",
        "jest": "24.9.0",
        "prettier": "1.18.2",
        "react": "16.9.0",
        "rimraf": "3.0.0",
        "ts-jest": "22.4.5",
        "ts-loader": "^6.1.0",
        "tslint": "5.20.0",
        "tslint-config-prettier": "1.18.0",
        "tslint-react": "4.0.0",
        "typescript": "3.6.4",
        "webpack": "^4.40.2",
        "webpack-cli": "^3.3.9"
    },
    "jest": {
        "testEnvironment": "node",
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "/tests/.*\\.spec\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
